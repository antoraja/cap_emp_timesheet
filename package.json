{
    "name": "emptimesheet",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap/cds": "^7",
        "@sap/cds-hana": "^2",
        "@sap/cds-odata-v2-adapter-proxy": "latest",
        "@sap/hana-client": "^2.17.22",
        "@sap/xsenv": "latest",
        "@sap/xssec": "3.3.2",
        "cds-swagger-ui-express": "^0.5.0",
        "express": "^4",
        "hana-cli": "^3.202404.1",
        "passport": "^0.4.1"
    },
    "devDependencies": {
        "@cap-js/sqlite": "^1",
        "@sap/cds-dk": "^7",
        "@sap/eslint-plugin-cds": "^3",
        "@sap/ux-specification": "^1.120.10",
        "eslint": "^9",
        "rimraf": "^3.0.2"
    },
    "scripts": {
        "start": "cds-serve",
        "watch-emptimesheetui": "cds watch --open emptimesheetui/webapp/index.html?sap-ui-xx-viewCache=false",
        "undeploy": "cf undeploy emptimesheet --delete-services --delete-service-keys --delete-service-brokers",
        "build": "rimraf resources mta_archives && mbt build --mtar archive",
        "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
    },
    "cds": {
        "requires": {
            "db": {
                "kind": "sql"
            },
            "uaa": {
                "kind": "hana"
            },
            "auth": {
                "[production]": {
                    "strategy": "JWT"
                }
            }
        },
        "hana": {
            "deploy-format": "hdbtable"
        },
        "features": {
            "fiori_preview": true
        }
    }
}
